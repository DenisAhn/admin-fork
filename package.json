{
  "name": "homy-admin-new",
  "version": "1.0.0",
  "main": "src/index.tsx",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint:es": "eslint --ignore-path .gitignore . --ext .ts,.tsx --max-warnings 0",
    "lint:es:fix": "eslint --fix --ignore-path .gitignore . --ext ts,.tsx",
    "format": "yarn lint:es:fix && yarn lint:style:fix",
    "lint:prettier": "prettier --ignore-path ./.prettierignore --config .prettierrc --check .",
    "lint:prettier:fix": "prettier --write .",
    "ts-check": "tsc --noEmit --project tsconfig.json",
    "precommit": "yarn run ts-check && yarn run lint:es && yarn run lint:prettier",
    "prepare-husky": "npx husky install && rm -f .husky/pre-commit && npx husky add .husky/pre-commit \"yarn ts-check && yarn precommit\""
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint . --ext .ts,.tsx --max-warnings 0"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not IE 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@iconify/react": "^4.1.1",
    "@material-ui/core": "^4.12.4",
    "@mui/lab": "^5.0.0-alpha.120",
    "@mui/material": "^5.13.3",
    "@mui/styles": "^5.13.2",
    "@mui/system": "^5.11.9",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "dayjs": "^1.11.8",
    "framer-motion": "^10.15.0",
    "history": "^5.3.0",
    "mobx": "^6.4.2",
    "mobx-react-lite": "^3.3.0",
    "qs": "^6.11.2",
    "query-string": "^8.1.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-lazy-load-image-component": "^1.6.0",
    "react-transition-group": "^4.4.5",
    "simplebar-react": "^3.2.4",
    "styled-components": "^6.0.5"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.4",
    "@types/react-lazy-load-image-component": "^1.5.3",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.48.2",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "@vitejs/plugin-legacy": "^3.0.1",
    "@vitejs/plugin-react": "^3.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.2.7",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-no-unsanitized": "^4.0.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-xss": "^0.1.12",
    "eslint-plugin-sort-class-members": "^1.18.0",
    "husky": "^8.0.1",
    "prettier": "^2.6.0",
    "react-router-dom": "^6.6.2",
    "sass": "^1.49.9",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.3",
    "vite": "^4.0.2",
    "vite-plugin-checker": "^0.5.2",
    "vite-plugin-environment": "^1.1.3",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-mkcert": "^1.13.2",
    "vite-plugin-svgr": "^2.4.0",
    "vite-tsconfig-paths": "^4.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx husky install && rm -f .husky/pre-commit && npx husky add .husky/pre-commit \"yarn ts-check && yarn precommit\""
    }
  }
}
